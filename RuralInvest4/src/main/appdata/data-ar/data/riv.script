CREATE CACHED TABLE PROJECT_BLOCK(BLOCK_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,PROJECT_ID INTEGER NOT NULL,DESCRIPTION VARCHAR(50),CYCLE_LENGTH INTEGER,CYCLE_PER_YEAR INTEGER,CYCLE_FIRST_YEAR INTEGER,UNIT_TYPE VARCHAR(50),WITH_WITHOUT INTEGER,LENGTH_UNIT INTEGER DEFAULT 0 NOT NULL,ORDER_BY INTEGER,CYCLE_FIRST_YEAR_INCOME INTEGER,CONSTRAINT PF_BLOCK_ID PRIMARY KEY(BLOCK_ID),CONSTRAINT FK_BLOCK_PROJECT FOREIGN KEY(PROJECT_ID) REFERENCES PROJECT(PROJECT_ID))
