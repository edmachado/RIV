CREATE CACHED TABLE REF_ITEM(REF_ITEM_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,PROJECT_ID INTEGER,PROFILE_ID INTEGER,ORDER_BY INTEGER NOT NULL,CLASS VARCHAR(10) NOT NULL,DESCRIPTION VARCHAR(50),UNIT_TYPE VARCHAR(50),UNIT_COST DECIMAL(7,2),TRANSPORT DECIMAL(7,2),CONSTRAINT PK_REF_ITEM_ID PRIMARY KEY(REF_ITEM_ID),CONSTRAINT FK_REFITEM_PROJID FOREIGN KEY(PROJECT_ID) REFERENCES PROJECT(PROJECT_ID),CONSTRAINT FK_REFITEM_PROFID FOREIGN KEY(PROFILE_ID) REFERENCES PROFILE(PROFILE_ID))
