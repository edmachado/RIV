CREATE CACHED TABLE APP_CONFIG(CONFIG_ID INTEGER NOT NULL PRIMARY KEY,CLASS VARCHAR(2) NOT NULL,DESCRIPTION VARCHAR(50) NOT NULL,INCOME_GEN BOOLEAN)
CREATE CACHED TABLE PROJECT(PROJECT_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,UNIQUE_ID BINARY(16) NOT NULL,CURRENCY VARCHAR(50),PROJECT_NAME VARCHAR(50),USER_CODE VARCHAR(20),SHARED BOOLEAN NOT NULL,WIZARD_STEP INTEGER,INCOME_GEN BOOLEAN NOT NULL,STARTUP_MONTH INTEGER,PROJ_CATEGORY INTEGER,BENEFICIARY INTEGER,STATUS INTEGER,WITH_WITHOUT BOOLEAN NOT NULL,EXCH_RATE DECIMAL(7,2),INFLATION_ANNUAL DECIMAL(7,2),PREP_DATE TIMESTAMP,LAST_UPDATE TIMESTAMP,FIELD_OFFICE INTEGER,LOCATION1 VARCHAR(50),LOCATION2 VARCHAR(50),LOCATION3 VARCHAR(50),TECHNICIAN INTEGER NOT NULL,BENEF_NAME VARCHAR(2000),BENEF_DESC VARCHAR(2000),DURATION INTEGER,PROJ_DESC VARCHAR(2000),BENE_DIRECT_NUM INTEGER,BENE_DIRECT_MEN INTEGER,BENE_DIRECT_WOMEN INTEGER,BENE_DIRECT_CHILD INTEGER,BENE_INDIRECT_NUM INTEGER,BENE_INDIRECT_MEN INTEGER,BENE_INDIRECT_WOMEN INTEGER,BENE_INDIRECT_CHILD INTEGER,JUSTIFICATION VARCHAR(2000),ACTIVITIES VARCHAR(2000),TECHNOLOGY VARCHAR(2000),REQUIREMENTS VARCHAR(2000),SUSTAINABILITY VARCHAR(2000),MARKET VARCHAR(2000),ENVIRO_IMPACT VARCHAR(2000),ORGANIZATION VARCHAR(2000),ASSUMPTIONS VARCHAR(2000),SOURCE_FUNDS VARCHAR(2000),CATEGORY_OTHER VARCHAR(50),LOAN1_INTEREST DECIMAL(7,2),LOAN1_DURATION INTEGER,LOAN1_GRACE_CAPITAL INTEGER,LOAN1_GRACE_INTEREST INTEGER,LOAN2_AMT DECIMAL(7,2),LOAN2_INTEREST DECIMAL(7,2),LOAN2_DURATION INTEGER,LOAN2_GRACE_CAPITAL INTEGER,LOAN2_GRACE_INTEREST INTEGER,LOAN2_INIT_PERIOD INTEGER,CAPITAL_INTEREST DECIMAL(7,2),CAPITAL_DONATE DECIMAL(7,2),RECC_DATE TIMESTAMP,RECC_CODE INTEGER,RECC_DESC VARCHAR(2000),ENVIRO_CATEGORY INTEGER,CAPITAL_OWN DECIMAL(7,2) DEFAULT 0,CREATED_BY VARCHAR(100),APP_CONFIG1 INTEGER DEFAULT -8,APP_CONFIG2 INTEGER DEFAULT -9,ADMIN_MISC1 VARCHAR(2000),ADMIN_MISC2 VARCHAR(2000),ADMIN_MISC3 VARCHAR(2000),CONSTRAINT PK_PROJECTID PRIMARY KEY(PROJECT_ID),CONSTRAINT UQ_UNIQUEID UNIQUE(UNIQUE_ID),CONSTRAINT FK_PROJECT_USER FOREIGN KEY(TECHNICIAN) REFERENCES USER(USER_ID),CONSTRAINT FK_PROJECT_OFFICE FOREIGN KEY(FIELD_OFFICE) REFERENCES APP_CONFIG(CONFIG_ID),CONSTRAINT FK_PROJECT_BENEF FOREIGN KEY(BENEFICIARY) REFERENCES APP_CONFIG(CONFIG_ID),CONSTRAINT FK_PROJECT_PROJCAT FOREIGN KEY(PROJ_CATEGORY) REFERENCES APP_CONFIG(CONFIG_ID),CONSTRAINT FK_ENVIROCAT_APPCONFIG FOREIGN KEY(ENVIRO_CATEGORY) REFERENCES APP_CONFIG(CONFIG_ID))
CREATE CACHED TABLE PROJECT_ITEM(PROJ_ITEM_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,CLASS VARCHAR(10) NOT NULL,PROJECT_ID INTEGER,DESCRIPTION VARCHAR(50),UNIT_TYPE VARCHAR(50),UNIT_NUM DECIMAL(10,2),UNIT_COST DECIMAL(7,2),DONATED DECIMAL(7,2),OWN_RESOURCES DECIMAL(7,2),ECON_LIFE INTEGER,MAINT_COST DECIMAL(7,2),SALVAGE DECIMAL(7,2),REPLACE BOOLEAN,YEAR_BEGIN INTEGER,PERSON_NUM INTEGER,CONTRIB_TYPE INTEGER,ORDER_BY INTEGER,LINKED_TO INTEGER,CONSTRAINT PK_PROJITEMID PRIMARY KEY(PROJ_ITEM_ID),CONSTRAINT FK_PROJECTITEM_PROJECT FOREIGN KEY(PROJECT_ID) REFERENCES PROJECT(PROJECT_ID))
